{
  "root": "libs/backend/events",
  "sourceRoot": "libs/backend/events/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/backend/events",
        "tsConfig": "libs/backend/events/tsconfig.lib.json",
        "packageJson": "libs/backend/events/package.json",
        "main": "libs/backend/events/src/index.ts",
        "assets": ["libs/backend/events/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/backend/events/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/backend/events"],
      "options": {
        "jestConfig": "libs/backend/events/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
